{
    "name": "karnoweb/laravel-module-manager",
    "description": "A powerful module management system for Laravel with dependency resolution, tree structure, and Laravel 12 support",
    "keywords": [
        "laravel",
        "module",
        "feature-flag",
        "dependency",
        "module-manager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Karnoweb",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Karnoweb\\LaravelModuleManager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Karnoweb\\LaravelModuleManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karnoweb\\LaravelModuleManager\\ModuleManagerServiceProvider"
            ],
            "aliases": {
                "Module": "Karnoweb\\LaravelModuleManager\\Facades\\Module"
            }
        }
    },
    "prefer-stable": true
}
